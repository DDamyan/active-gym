generator client {
   provider = "prisma-client-js"
}

datasource db {
   provider = "mysql"
   url      = "mysql://root:root@localhost:3306/test_schema"
}

model User {
   id    Int     @id @unique @default(autoincrement())
   email String  @unique
   name  String?
